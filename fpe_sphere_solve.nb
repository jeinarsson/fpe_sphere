Cell["\<\
Extract angular averages etc from the coefficients\
\>", "Section",
 CellChangeTimes->{{3.57935088591253*^9, 3.579350899008279*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"c00idx", " ", "=", " ", 
   RowBox[{"sparseindex", "[", 
    RowBox[{"0", ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c20idx", " ", "=", " ", 
   RowBox[{"sparseindex", "[", 
    RowBox[{"2", ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c22imidx", " ", "=", " ", 
   RowBox[{
    RowBox[{"sparseindex", "[", 
     RowBox[{"2", ",", "2"}], "]"}], "+", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c40idx", " ", "=", " ", 
   RowBox[{"sparseindex", "[", 
    RowBox[{"4", ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"c44idx", " ", "=", " ", 
   RowBox[{"sparseindex", "[", 
    RowBox[{"4", ",", "4"}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.579351075576378*^9, 3.5793511080632358`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"sinSqrTheta", "[", "x_", "]"}], ":=", 
   RowBox[{"1", "-", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       FractionBox["2", "3"], " ", 
       SqrtBox["\[Pi]"], 
       RowBox[{"x", "[", 
        RowBox[{"[", "c00idx", "]"}], "]"}]}], "+", 
      RowBox[{
       FractionBox["4", "3"], " ", 
       SqrtBox[
        FractionBox["\[Pi]", "5"]], 
       RowBox[{"x", "[", 
        RowBox[{"[", "c20idx", "]"}], "]"}]}]}], ")"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.5793511309445443`*^9, 3.5793511608652563`*^9}}]
}, Open  ]]
}, Open  ]],

